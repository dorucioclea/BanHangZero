  </ItemGroup>

  <ItemGroup>
    <Reference Include="Center.Production.Core">
      <HintPath>..\Center.Production.Core\bin\Debug\netcoreapp2.2\Center.Production.Core.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="NETStandard.Library" Version="2.0.3" />
  </ItemGroup>

</Project>
